code = "shader_type canvas_item;

uniform float frame;
uniform sampler2D gradient : hint_black;

void fragment() {
    vec4 temp = texture(TEXTURE,UV);
	float f = round(frame*64.0);
	vec2 offset = vec2(mod(f,8.0)/8.0,float(int(f)/8)/8.0);
	vec2 uv = (UV - offset) * 8.0;
	
	/*
	float cutoff = mod(frame+0.125, 0.250)*4.0;
	float r,g,b;
	if (uv.x < cutoff) {
		r = 1.0;
	}
	else {
		r = 0.0;
	}
	r = temp.r * uv.y;
	g = temp.g * (1.0 - uv.y);
	b = temp.g * (1.0 - uv.y);
	*/
	
	vec3 sample = texture(gradient, vec2(uv.y,0.0)).rgb;
	
	COLOR = vec4(mix(temp.rgb,sample,0.66),temp.w);
}"

